//part - 01

// #include<stdio.h>
// int main()
// {
//     int a,i,j,m,n;
//     printf("Enter Your Number: ");
//     scanf("%d",&a);
//     for(i=1; i<=a; i++)
//     {
//         for(j=1; j<=i; j++)
