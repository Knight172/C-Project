//print pattern in c   06
//part - 01


// #include<stdio.h>
// int main()
// {
//     int a,i,u,m;
//     printf("Enter Your Number: ");
//     scanf("%d",&a);
//     for(i=1; i<=a; i++)
//     {
//         for(u=1; u<=a-i; u++)
//         {
//             printf(" ",u);
//         }
//         for(m=1; m<=i; m++)
//         {
//             printf("%d",m);
//         }
//         printf("\n");
//     }
// }

//part - 02

